<!DOCTYPE hibernate-mapping PUBLIC  
 "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">  
  
 <hibernate-mapping>  
  <class name="com.payroll.employee.lic.dataobjects.EmpLic" table="EMP_LIC">  
    <id name="empId" type="integer" >  
     <column name="EMPID" sql-type="integer" not-null="true"/>  
    </id>  
    <property name="paymentDate" column="PAYMENT_DATE" not-null="true"/> 
    <property name="policyNo" column="POLICY_NO" not-null="true"/> 
    <property name="instlmtAmt" column="INSTALL_AMT" not-null="true"/> 
    <property name="status" column="STATUS" not-null="true"/>  
    <property name="rowUpdDate" column="ROW_UPD_DATE" not-null="true"/> 
  </class>  
 </hibernate-mapping> 