<!DOCTYPE hibernate-mapping PUBLIC  
 "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">  
  
 <hibernate-mapping>  
  <class name="com.payroll.designation.dataobjects.DesignationHead" table="DESG_HEAD">  
    <composite-id> 
     	<key-property name="designationId" column="DESG_ID" />  
		<key-property name="headId" column="HEAD_ID" />  
    </composite-id>  
    <property name="startDate" column="START_DATE" not-null="true"/> 
  </class>  
 </hibernate-mapping> 